{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node@2.15.1"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "installCommand": "npm install",
  "outputDirectory": "public",
  "public": true,
  "nodeVersion": "18.x"
}
