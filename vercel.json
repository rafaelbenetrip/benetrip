{
  "version": 2,
  "functions": {
    "api/recommendations.js": {
      "memory": 1024,
      "maxDuration": 300
    },
    "api/flight-search.js": {
      "memory": 1024,
      "maxDuration": 90
    },
    "api/image-search.js": {
      "memory": 1024,
      "maxDuration": 90
    },
    "api/flight-results.js": {
       "memory": 1024,
       "maxDuration": 90
    },
    "api/flight-redirect.js": {
      "memory": 1024,
      "maxDuration": 60
    }, 
    "api/itinerary-generator.js": {
      "memory": 1024,
      "maxDuration": 180
    },
    "api/weather.js": {
      "memory": 1024,
      "maxDuration": 180
    },
    "api/cheapest-flights.js": {
      "memory": 1024,
      "maxDuration": 90
    },
    "api/search-destinations.js": {
      "memory": 1024,
      "maxDuration": 90
    }
  },
  "routes": [
    {
      "src": "/assets/js/(.*)\\.js",
      "headers": {
        "Content-Type": "application/javascript; charset=utf-8",
        "Access-Control-Allow-Origin": "*",
        "Cache-Control": "public, max-age=86400",
        "X-Content-Type-Options": "nosniff"
      },
      "continue": true
    },
    {
      "src": "/assets/css/(.*)\\.css",
      "headers": {
        "Content-Type": "text/css; charset=utf-8",
        "Access-Control-Allow-Origin": "*",
        "Cache-Control": "public, max-age=86400"
      },
      "continue": true
    },
    {
      "src": "/api/weather",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Content-Type": "application/json; charset=utf-8",
        "X-Content-Type-Options": "nosniff",
        "Cache-Control": "public, max-age=300, stale-while-revalidate=86400"
      },
      "dest": "/api/weather.js"
    },
    {
      "src": "/api/itinerary-generator",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Content-Type": "application/json; charset=utf-8",
        "X-Content-Type-Options": "nosniff"
      },
      "dest": "/api/itinerary-generator.js"
    },
    {
      "src": "/api/flight-results",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Content-Type": "application/json; charset=utf-8",
        "X-Content-Type-Options": "nosniff"
      },
      "dest": "/api/flight-results.js"
    },
    {
      "src": "/api/flight-redirect",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Content-Type": "application/json; charset=utf-8",
        "X-Content-Type-Options": "nosniff"
      },
      "dest": "/api/flight-redirect.js"
    },
    {
      "src": "/api/recommendations",
      "dest": "/api/recommendations.js"
    },
    {
      "src": "/api/flight-search",
      "dest": "/api/flight-search.js"
    },
    {
      "src": "/api/image-search",
      "dest": "/api/image-search.js"
    },
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Content-Type": "application/json",
        "Connection": "keep-alive",
        "Keep-Alive": "timeout=60",
        "Cache-Control": "public, max-age=60, stale-while-revalidate=300"
      },
      "continue": true
    },
    {
      "src": "/destinos",
      "dest": "/public/destinos.html"
    },
    {
      "src": "/itinerary",
      "dest": "/public/itinerary.html"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Cache-Control": "public, max-age=86400"
      },
      "dest": "/public/assets/$1"
    },
    {
      "src": "/(.*)\\.html",
      "dest": "/public/$1.html"
    },
    {
      "src": "/$",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
